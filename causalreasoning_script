                },
                {
                    "id": "vignettechoice3",
                    "kind": "exp-lookit-images-audio",
                    "baseDir": "https://raw.githubusercontent.com/lleotti/lookit-stimuli-template/master/",
                    "audio": AUDIOQ3,
                    "audioTypes": [
                        "mp3"
                    ],
                    "images": [{
                            "src": ENDIMG,
                            "top": "1",
                            "left": "15",
                            "displayDelayMs": 0,
                            "nonChoiceOption": true
                        },
                        {
                            "id": "choice1",
                            "src": CHOICE1,
                            "top": "70",
                            "left": "2",
                            "width": "20",
                            "height": "30",
                            "displayDelayMs": 2000
                        },
                        {
                            "id": "choice2",
                            "src": CHOICE2,
                            "top": "70",
                            "left": "26",
                            "width": "20",
